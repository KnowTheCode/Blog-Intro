{
    "name" 			: "knowthecode/blog-introduction",
    "description" 	: "Introduce your blog page to your audience using the built-in WordPress editor.",
    "type"          : "project",
    "keywords" 		: ["wordpress", "wp", "plugin"],
    "homepage" 		: "https://knowthecode.io",
    "license" 		: "GPL-2.0+",
    "authors": [
        {
            "name" 	: "hellofromTonya",
            "email" : "hellofromtonya@knowthecode.io"
        }
    ],
    "require": {
        "php" : ">=5.4.0"
    },
    "require-dev": {
        "raveren/kint" : "~1.0",
        "filp/whoops": "~2.0"
    },
    "autoload" : {
        "classmap": [],
        "files": [
            "src/admin/enable-editor.php",
            "src/contents.php"
        ]
    },
    "autoload-dev" : {
        "files": [
            "src/support/exceptions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "vendor-dir": "assets/vendor"
    },
    "minimum-stability": "dev"
}